<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vst.recyclerviewdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.HeaderRvActivity" />
        <activity android:name=".ui.MultiltemRvActivity" />
        <activity android:name=".ui.PullUpRvActivity" />
        <activity android:name=".ui.RefreshRvActivity" />
        <activity android:name=".ui.OMultilActivity" />
    </application>

</manifest>